         DAY~((Q|26-2) DIV 10+DATE+P+P.[36:10]+1) MOD 7;                77048000          
         WRITE(LP,HEADFMT,DAYS[DAY],                                    77049000          
                          MONTHS[MONTH|2],MONTHS[MONTH|2+1],DATE,YEAR,  77050000          
                          I~HOURS MOD 12, MINUTES,                      77051000          
                          IF HOURS>12 THEN "P" ELSE "A");               77052000          
