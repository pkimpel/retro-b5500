         IF PSR < 0                                                     04027000          
         THEN BEGIN                                                     04028000          
                   PSR ~ PSR + ABS(N);                                  04029000          
                   DO PIR ~ PIR - 1 UNTIL N ~ N + 6 } 0;                04030000          
