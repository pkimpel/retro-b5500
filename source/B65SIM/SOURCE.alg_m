                   "LP  ","LP  ", "NONE", "CR  ", "CP  ", 0,0,"TAPE";   77054000          
         WRITE (LP, CONF, CONL);                                        77055000          
         WRITE (LP, CABH);                                              77056000          
         FILL NAMES [0, *] WITH                                         77057000          
              "NONE  ","DISKA ","SPO   ","AIDS  ","NONE  ","NONE  " ,   77058000          
              "LP    ","LP    ","NONE  ","CARD  ","PUNCH ",0,0,"TAPEA ";77059000          
