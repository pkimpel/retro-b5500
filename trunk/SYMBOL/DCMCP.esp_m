SAVE PROCEDURE ESPBIT; COMMENT PRESENCE BIT ROUTNE FOR ESP SEGMENTS ;%  00025900        
         BEGIN INTEGER PRTLOC,SYLLABLE,LOC,SIZE;%                       00026000        
               FIELD MAYBEWORKEDON = [7:1]; %                      %156-00027000        
               ARRAY MYSELF=ESPBIT[*];%                                 00028000        
