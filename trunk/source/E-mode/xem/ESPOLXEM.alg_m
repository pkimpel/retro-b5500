    THRU CNT DO                                                         08046000=02001866=
      STREAMSMEARLIT(1, 6"1");          % IN CASE A CHAR=A,B,C,D,OR F.  08048000          
                                                                        08050000          
    STREAMSETSI(S, SX);                                                 08052000=02001868=
