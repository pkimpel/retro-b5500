VALUE P,Y;INTEGER P;REAL Y;                                             00202000
   BEGIN OWN INTEGER N;REAL V;                                          00203000
DEFINE PR(PR1)=(PR1.[41:19])#;                                          00204000
FORMAT F1(I4,": ",A4,I6,", ",A4,I6);                                    00205000
