   LABEL B3,B4,B5,B6,B7,B8,B9;                                          00220000
  LABEL B10,QDUMP,MEMDP,MEMLD,QIN,QSET;                                 00221000
   INTEGER I,K,IS;                                                      00222000
   ALPHA XS;                                                            00223000
